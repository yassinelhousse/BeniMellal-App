{
  "expo": {
    "name": "tourist-app",
    "slug": "tourist-app",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "scheme": "touristapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {

      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.touristapp"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
